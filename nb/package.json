{
  "name": "skyltdaxwp",
  "version": "1.0.0",
  "description": "woo commerce theme with custom product generator and custom cart items",
  "main": "app.js",
  "scripts": {
    "scss": "node-sass --watch assets/scss -o assets/dist/css",
    "build-css": "node-sass -o assets/dist/css/ assets/scss/",
    "watch-css": "nodemon -e scss -x  \"npm run build-css\"",
    "build": "webpack --mode production",
    "watch-build": "webpack --watch --mode production",
    "dev": "webpack --watch --mode production && nodemon -e scss -x  \"npm run build-css\"",
    "test": "eslint assets/src",
    "lint": "npx stylelint 'assets/dist/css/*.css'"
  },
  "author": "Martin Ekelund",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "babel-core": "^4.7.16",
    "babel-loader": "^9.1.2",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "file-loader": "^6.2.0",
    "node-sass": "^8.0.0",
    "nodemon": "^2.0.4",
    "sass": "^1.62.1",
    "sass-loader": "^13.3.1",
    "stylelint": "^15.10.3",
    "stylelint-config-standard": "^34.0.0",
    "vue-loader": "^15.10.1",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.85.1",
    "webpack-cli": "^5.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Kidkie-Tech-AB/skyltdax-theme.git"
  },
  "homepage": "https://skyltdax.hemsida.eu",
  "dependencies": {
    "acorn-import-assertions": "^1.9.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "fabric": "^5.3.0",
    "html-webpack-plugin": "^5.5.3",
    "vue": "^2.7.14"
  }
}
